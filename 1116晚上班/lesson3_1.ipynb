    "#if單項選擇\n",
    "#請輸入學生分數0~100,如果超過100,就給100,如果小於0,最小為0\n",
    "\n",
    "scores = int(input(\"請輸入學生分數:\"))\n",
    "if scores > 100:\n",
    "    scores = 100\n",
    "    \n",
    "if scores < 0:\n",
    "    scores = 0\n",
    "\n",
    "print(f\"學生分數是:{scores}\")"
